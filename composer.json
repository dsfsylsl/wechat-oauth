{
    "name": "gxyshs/wechat-oauth",
    "description": "微信授权服务",
    "license": "MIT",
    "authors": [
        {
            "name": "fengshiyu",
            "email": "5722148870@qq.com",
            "homepage": "https://github.com/gxyshs/wechat-oauth"
        }
    ],
    "homepage": "https://github.com/gxyshs/wechat-oauth",
    "keywords": ["Laravel", "WechatOauth"],
    "require": {
        "php": ">=7.4",
        "illuminate/support": "~7|~8",
        "overtrue/wechat": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Gxyshs\\WechatOauth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gxyshs\\WechatOauth\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gxyshs\\WechatOauth\\WechatOauthServiceProvider"
            ],
            "aliases": {
                "WechatOauth": "Gxyshs\\WechatOauth\\Facades\\WechatOauth"
            }
        }
    }
}
